def linear_search(items, inp):
    found = False
    for x in range(len(items)):
        if items[x] == inp:
            print (f"item {inp} is found at {x}")
            found = True
            break
    if found:
            print(f"{inp} is in the list")
    else:
            print("data item not found.")

items = [5,10,15,20,25,30,35,40,45,50]
inp = 20
linear_search (items, inp)

